@model CarRepairShop.web.Models.VehicleViewModel

@{
    ViewData["Title"] = "Create new Vehicle";
}

<h1>Create new vehicle</h1>

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" enctype="multipart/form-data">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>

            <partial name="_Vehicles" />

            <div class="form-group">
                <button type="submit" value="Create" class="btn btn-primary"><i class="fa fa-save"></i> Create</button>
                <a asp-action="Index" class="btn btn-success"><i class="fa fa-chevron-left"></i>Back to List</a>
            </div>
        </form>
    </div>
</div>

<div>

    <a asp-action="Index" class="btn btn-success">Back to List</a>

</div>




@section Scripts {

    @{

        await Html.RenderPartialAsync("_ValidationScriptsPartial");

    }

}